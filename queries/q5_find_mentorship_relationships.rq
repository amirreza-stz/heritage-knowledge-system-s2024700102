# Query 5: Find mentorship relationships
# Shows who mentors whom in the cultural community

PREFIX : <http://www.semanticweb.org/ubuntu/ontologies/2026/0/s2024700102heritage/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?mentor ?mentorLabel ?apprentice ?apprenticeLabel
WHERE {
    ?apprentice :mentoredBy ?mentor .
    ?mentor rdfs:label ?mentorLabel .
    ?apprentice rdfs:label ?apprenticeLabel .
}
ORDER BY ?mentorLabel
