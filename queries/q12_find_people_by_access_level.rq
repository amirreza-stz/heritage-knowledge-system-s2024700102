# Query 12: Find people by access level
# Lists all people grouped by their access level

PREFIX : <http://www.semanticweb.org/ubuntu/ontologies/2026/0/s2024700102heritage/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?accessLevel ?person ?personLabel ?humanApproval
WHERE {
    ?person rdf:type :Person .
    ?person :hasAccessLevel ?accessLevel .
    ?person rdfs:label ?personLabel .
    OPTIONAL { ?person :humanApproval ?humanApproval }
}
ORDER BY ?accessLevel ?personLabel
