# Query 3: Find all claims with confidence > 0.8 (RDF-star pattern)
# For dataset: heritage-rdfstar

PREFIX ex: <http://example.org/heritage/s2024700102#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

SELECT ?ritual ?cycle ?source ?confidence ?dateRecorded
WHERE {
    << ?ritual ex:belongsTo ?cycle >>
        ex:source ?source ;
        ex:confidence ?confidence ;
        ex:dateRecorded ?dateRecorded .
    FILTER (?confidence > 0.8)
}
ORDER BY DESC(?confidence)
