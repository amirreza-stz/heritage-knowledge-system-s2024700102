@prefix : <http://www.semanticweb.org/ubuntu/ontologies/2026/0/s2024700102heritage/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ex: <http://www.semanticweb.org/ubuntu/ontologies/2026/0/s2024700102heritage/claim/> .

#################################################################
# Contested Claims - Named Graphs Pattern (TriG format)
# Question 2: Multi-Perspective Knowledge Representation
# 
# Different communities have conflicting claims about which cycle
# cultural rituals belong to (Lunar Cycle vs Solar Cycle).
# This file uses named graphs to represent these contested claims
# with provenance metadata.
#################################################################

# Cycle classes
:LunarCycle rdf:type rdfs:Class ;
            rdfs:label "Lunar Cycle"@en ;
            rdfs:label "Ay Dövrü"@az .

:SolarCycle rdf:type rdfs:Class ;
            rdfs:label "Solar Cycle"@en ;
            rdfs:label "Günəş Dövrü"@az .

# Source communities/tribes
:TribeA rdf:type :Community ;
        rdfs:label "Tribe A - Ashiq Tradition"@en ;
        rdfs:label "Aşiq Ənənəsi - A Tayfası"@az .

:TribeB rdf:type :Community ;
        rdfs:label "Tribe B - Shaman Tradition"@en ;
        rdfs:label "Şaman Ənənəsi - B Tayfası"@az .

:TribeC rdf:type :Community ;
        rdfs:label "Tribe C - Academic Research"@en ;
        rdfs:label "Akademik Tədqiqat - C Tayfası"@az .

# Properties for metadata
:belongsTo rdf:type owl:ObjectProperty ;
           rdfs:domain :Ritual ;
           rdfs:range rdfs:Class .

:source rdf:type owl:ObjectProperty ;
        rdfs:range :Community .

:confidence rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:decimal .

:dateRecorded rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:date .

# Named Graph 1: Spring Ritual - Tribe A claims Lunar
<ex:s2024700102_claim/tribeA/001> {
    :SpringRitual :belongsTo :LunarCycle .
    <ex:s2024700102_claim/tribeA/001> :source :TribeA ;
                                       :confidence 0.9 ;
                                       :dateRecorded "2024-03-15"^^xsd:date .
}

# Named Graph 2: Spring Ritual - Tribe B claims Solar
<ex:s2024700102_claim/tribeB/001> {
    :SpringRitual :belongsTo :SolarCycle .
    <ex:s2024700102_claim/tribeB/001> :source :TribeB ;
                                       :confidence 0.85 ;
                                       :dateRecorded "2024-03-20"^^xsd:date .
}

